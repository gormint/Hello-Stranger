
<div id="map" class="hide"></div>
<div class="pure-u-11-12" id="list-events-ul"></div>

<div class="pure-u-11-12" id="historical-list-events-ul"></div>

<div class="pure-u-1-12 pure-u-md-1-12 pure-u-lg-2-12"></div>
<div class="pure-u-10-12 pure-u-md-10-12 pure-u-lg-8-12"></div>
<div class="pure-u-1-12 pure-u-md-1-12 pure-u-lg-2-12"></div>


  <% for(i=0; i<events.length; i++){ %>
    <div class="pure-g" data-eventObjectId="{{ eventObjectId }}">
      <div class="pure-u-3-4 list-info">
        <ul>
        <a href="/events/{{ eventObjectId }}">
          <li class="title">{{ title }}</li>
          <li class="name">{{ name }}</li>
        </a>
        </ul>
      </div>
      <div class="pure-u-1-4 list-date">
        <a href="/events/{{ eventObjectId }}">
          <p>{{ startDate }}</p>
        </a>
      </div>
    </div>
  <% } %>
   

<script type="text/javascript" src="/js/pastevents.js"></script>